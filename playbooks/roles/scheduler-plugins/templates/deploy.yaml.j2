apiVersion: apps/v1
kind: Deployment
metadata:
  name: pause
  namespace: scheduler-plugins
spec:
  replicas: 2
  selector:
    matchLabels:
      app: pause
  template:
    metadata:
      labels:
        app: pause
        scheduling.x-k8s.io/pod-group: pg1
    spec:
      containers:
        - name: pause
          image: "{{ pause_image }}"

